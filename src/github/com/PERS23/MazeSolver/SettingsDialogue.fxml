<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<fx:root hgap="8.0" type="GridPane" vgap="4.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <Label text="%maze_width_setting_label" />
        <TextField fx:id="maze_width_entry" GridPane.columnIndex="1" GridPane.columnSpan="2" />
        <Label text="%maze_height_setting_label" GridPane.rowIndex="1" />
        <TextField fx:id="maze_height_entry" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="1" />
        <Label text="%corridor_size_setting_label" GridPane.rowIndex="2" />
        <TextField fx:id="corridor_size_entry" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="2" />
        <Label text="%wall_size_setting_label" GridPane.rowIndex="3" />
        <TextField fx:id="wall_size_entry" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="3" />
        <Label text="%starting_x_point_setting_label" GridPane.rowIndex="4" />
        <TextField fx:id="starting_x_entry" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="4" />
        <Label text="%starting_y_point_setting_label" GridPane.rowIndex="5" />
        <TextField fx:id="starting_y_entry" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="5" />
        <Label text="%ending_x_point_setting_label" GridPane.rowIndex="6" />
        <TextField fx:id="ending_x_entry" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="6" />
        <Label text="%ending_y_point_setting_label" GridPane.rowIndex="7" />
        <TextField fx:id="ending_y_entry" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="7" />
        <Button fx:id="ok_button" mnemonicParsing="false" onAction="#handleOkButton" text="%ok_button" GridPane.columnIndex="1" GridPane.rowIndex="8" GridPane.halignment="RIGHT"/>
    </children>
    <padding>
        <Insets bottom="32.0" left="32.0" right="32.0" top="32.0" />
    </padding>
</fx:root>
